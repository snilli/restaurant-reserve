	"packageManager": "pnpm@9.3.0",
	"main": "dist/main.js",
	"engines": {
		"node": "20.x"
	},
	"type": "commonjs",
	"scripts": {
		"build": "nest build",
		"build:migration": "nest build migration",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
		"start": "nest start",
		"dev": "nest start --watch",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"linta": "eslint \"{src,apps,libs,test}/**/*.spec.ts\" --fix",
		"test": "vitest run",
		"test:e2e": "vitest run --config ./test/vitest.config.e2e.mts",
