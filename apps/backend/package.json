		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"prettier": "^3.3.3",
		"supertest": "^7.0.0",
		"swc-loader": "^0.2.6",
		"ts-loader": "^9.5.1",
